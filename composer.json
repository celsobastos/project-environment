{
    "name": "celso/environment",
    "description": "Create the a project using the most news technologies",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Celso\\Environment\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "require": {
        "drupal/coder": "^8.3",
        "vlucas/phpdotenv": "^5.5",
        "symfony/dotenv": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
